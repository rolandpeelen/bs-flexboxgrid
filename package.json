{
  "name": "bs-flexboxgrid",
  "version": "2.0.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "server": "webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{re, rei}": [
      "bsrefmt --in-place",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "RWJ Peelen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rolandpeelen/bs-flexboxgrid"
  },
  "dependencies": {
    "flexboxgrid": "^6.3.1"
  },
  "devDependencies": {
    "bs-css": "^11.0.0",
    "bs-platform": "^8.0.2",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reason-react": ">=0.9.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
